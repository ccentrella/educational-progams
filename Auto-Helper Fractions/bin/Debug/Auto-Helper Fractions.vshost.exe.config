                <value>True</value>
            </setting>
            <setting name="ReduceRequired" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="Theme" serializeAs="String">
                <value>Standard</value>
            </setting>
        </AutoHelper.My.MySettings>
